        },
        {
          "text": "Alert",
          "value": 0,
          "type": "Select",
          "options": [
            {
              "text": "None",
              "value": "0"
            },
            {
              "text": "Left",
              "value": "1"
            },
            {
              "text": "Right",
              "value": "2"
            },
            {
              "text": "Both",
              "value": "3"
            }
